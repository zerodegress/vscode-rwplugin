{
  "name": "rwiniplugin",
  "displayName": "RwiniPlugin",
  "description": "A plugin which improves the coding for MODs of a RTS game RustedWarfare.",
  "publisher": "zerodegress",
  "icon": "rwp.png",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:ini"
  ],
  "main": "./dist/extension.min.cjs",
  "contributes": {
    "languages": [
      {
        "id": "rwini",
        "aliases": [
          "rwini"
        ],
        "extensions": [
          ".ini"
        ],
        "configuration": "./language-configuration.json",
        "editor.semanticHighlighting.enabled": true
      }
    ],
    "semanticTokenTypes": [
      {
        "id": "customVariable",
        "superType": "customVariable",
        "description": "Defined by unit memory."
      }
    ],
    "grammars": [
      {
        "language": "rwini",
        "scopeName": "source.ini",
        "path": "./syntaxes/rwini.tmLanguage.json"
      }
    ],
    "commands": []
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.49.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "rollup": "^3.29.0",
    "xlsx": "^0.18.5"
  }
}
